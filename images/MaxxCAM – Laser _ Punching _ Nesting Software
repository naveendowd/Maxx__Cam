<script>
  const materialSpend = document.getElementById("material-spend");
  const materialSaving = document.getElementById("material-saving");
  const hourlyRate = document.getElementById("hourly-rate");
  const timeSaving = document.getElementById("time-saving");
  const machineRate = document.getElementById("machine-rate");
  const runtimeSaving = document.getElementById("runtime-saving");
  const resultsSection = document.getElementById("results");
  const settingExplained = document.getElementById(
    "calculator-setting-explained"
  );
  const currencyOptions = document.querySelectorAll('input[name="currency"]');
  const calculateButton = document.getElementById("calculate-button");

  function calculateResults() {
    const materialSpendValue = parseFloat(materialSpend.value);
    const materialSavingValue = parseFloat(materialSaving.value) / 100;
    const hourlyRateValue = parseFloat(hourlyRate.value);
    const timeSavingValue = parseFloat(timeSaving.value) / 100;
    const machineRateValue = parseFloat(machineRate.value);
    const runtimeSavingValue = parseFloat(runtimeSaving.value) / 100;

    // Updating ToolTips
    document.getElementById("tooltip-material-spend").textContent =
      formatCurrency(materialSpendValue);
    document.getElementById("tooltip-material-saving").textContent = `${
      materialSavingValue * 100
    }% `;

    document.getElementById("tooltip-hourly-rate").textContent =
      formatCurrency(hourlyRateValue);
    document.getElementById("tooltip-time-saving").textContent = `${
      timeSavingValue * 100
    }%`;

    document.getElementById("tooltip-machine-rate").textContent =
      formatCurrency(machineRateValue);
    document.getElementById("tooltip-runtime-saving").textContent = `${
      runtimeSavingValue * 100
    }%`;

    const materialResult = materialSpendValue * materialSavingValue;
    const timeResult = hourlyRateValue * 40 * 52 * timeSavingValue;
    const runtimeResult = machineRateValue * 40 * 52 * runtimeSavingValue;
    const totalResult = materialResult + timeResult + runtimeResult;

    document.getElementById("material-result").innerText =
      formatCurrency(materialResult);
    document.getElementById("time-result").innerText =
      formatCurrency(timeResult);
    document.getElementById("runtime-result").innerText =
      formatCurrency(runtimeResult);
    document.getElementById("total-result").innerText =
      formatCurrency(totalResult);

    resultsSection.style.display = "flex";
    settingExplained.style.display = "none";
  }

  function formatCurrency(value) {
    const selectedCurrency = document.querySelector(
      'input[name="currency"]:checked'
    ).value;
    const symbols = { USD: "$", EURO: "€", POUND: "£" };
    return `${symbols[selectedCurrency]}${value.toLocaleString()}`;
  }

  function updateSliderValues() {
    document.getElementById("material-spend-value").innerText = formatCurrency(
      parseFloat(materialSpend.value)
    );
    document.getElementById("material-saving-value").innerText =
      materialSaving.value;
    document.getElementById("hourly-rate-value").innerText = formatCurrency(
      parseFloat(hourlyRate.value)
    );
    document.getElementById("time-saving-value").innerText = timeSaving.value;
    document.getElementById("machine-rate-value").innerText = formatCurrency(
      parseFloat(machineRate.value)
    );
    document.getElementById("runtime-saving-value").innerText =
      runtimeSaving.value;
  }

  // function requestDemo() {
  //   alert("Request Demo or Quote feature coming soon!");
  // }

  // Event listeners
  materialSpend.addEventListener("input", updateSliderValues);
  materialSaving.addEventListener("input", updateSliderValues);
  hourlyRate.addEventListener("input", updateSliderValues);
  timeSaving.addEventListener("input", updateSliderValues);
  machineRate.addEventListener("input", updateSliderValues);
  runtimeSaving.addEventListener("input", updateSliderValues);

  currencyOptions.forEach((option) => {
    option.addEventListener("change", updateSliderValues);
  });

  calculateButton.addEventListener("click", () => {
    calculateResults();
  });

  resultsSection.style.display = "none";
</script>
<script type="module" src="./MaxxCAM – Laser _ Punching _ Nesting Software_files/ionicons.esm.js.download"></script>
<script nomodule="" src="./MaxxCAM – Laser _ Punching _ Nesting Software_files/ionicons.js.download"></script>

		</div>
	</div>
